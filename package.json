{
  "name": "expressjs-typescript",
  "version": "1.0.0",
  "description": "Expressjs using typescript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "dev": "env-cmd --file .env nodemon index.ts",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint-fix": "tslint -c tslint.json -p tsconfig.json --fix"
  },
  "keywords": ["node", "typescript", "expressjs"],
  "author": "Somnath Official",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.21",
    "env-cmd": "^10.1.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.20.0"
  }
}
